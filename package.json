  "scripts": {
    "dev": "next dev --turbo",
    "build": "tsx lib/db/migrate && next build",
    "start": "next start",
    "lint": "next lint && biome lint --write --unsafe",
    "lint:fix": "next lint --fix && biome lint --write --unsafe",
    "format": "biome format --write",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx lib/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "fetch-models": "tsx scripts/fetch-models.ts",
    "fetch-model-features": "tsx scripts/fetch-model-features.ts",
    "test": "export PLAYWRIGHT=True && npx playwright test --workers=4",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:unit:watch": "vitest --config vitest.config.ts",
    "test:components": "vitest run --config vitest.config.unit.ts",
    "test:components:watch": "vitest --config vitest.config.unit.ts",
    "test:all": "vitest run --config vitest.config.ts && vitest run --config vitest.config.unit.ts",
    "test:types": "next typegen . && tsc --noEmit",
    "test:db": "tests/db/scripts/run-simple-tests.sh",
    "test:db:ci": "tests/db/scripts/ci-test.sh",
    "test:db:start": "docker-compose -f docker-compose.test.yml up -d postgres-test",
    "test:db:stop": "docker-compose -f docker-compose.test.yml down -v",
    "test:db:setup": "tests/db/scripts/run-tests.sh setup",
    "test:db:clean": "tests/db/scripts/run-tests.sh clean",
    "test:db:shell": "tests/db/scripts/run-tests.sh shell",
    "test:db:logs": "tests/db/scripts/run-tests.sh logs",
    "test:db:file": "tests/db/scripts/run-tests.sh -f",
    "test:db:supabase": "vitest run tests/db/supabase-example.test.ts",
    "test:db:pgtap": "tests/db/scripts/run-pgtap.sh",
    "db:test:migrate": "DATABASE_URL=postgresql://test_user:test_password@localhost:5433/test_db npx tsx lib/db/migrate.ts"
